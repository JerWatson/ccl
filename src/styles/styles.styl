// primary palette
// #0768A9
blue  = rgb(  7, 104, 169)
// #006A3A
green = rgb(  0, 106,  58)
// #2E2D27
black = rgb( 46,  45,  39)
// #FFFFFF
white = rgb(255, 255, 255)

// secondary palette
// #FF850D
saffron   = rgb(255, 133,  13)
// #5C005D
royal     = rgb( 92,   0,  93)
// #C9105C
carnation = rgb(201,  16,  92)
// #FFD100
dandelion = rgb(255, 209,   0)

body
  background black

h1,
h2,
h3,
h4,
h5,
h6,
.lead
  font-family "News Gothic Demi", "Helvetica Neue", Helvetica, Arial, sans-serif

.lead
  font-size 18px

.text-success
  color green
.text-info
  color saffron
.text-warning
  color royal
.text-danger
  color carnation

.navbar
  margin-bottom 0

.navbar-inverse .navbar-nav > li > a
  color #ccc

#header
  background #f5f5f5
  padding-top 50px
  border-bottom 1px solid #ddd
  h2
    font-family "News Gothic Bold"
    line-height 90px
    margin 0
    white-space nowrap
    overflow hidden
    a
      display inline-block
    span
      font-family "News Gothic Demi"

.heading
  line-height 90px
  // padding-top 15px
  // padding-bottom 15px

#main
  background white
  padding 30px 0

#aside
  background white
  .row
    border-top 1px solid #eee
    padding 30px 0

#footer
  background url("/assets/imgs/layout/dancing-squares.png") no-repeat right center
  color white
  padding 30px 0
  a
    color white

.page-content
  margin-bottom 2em

.breadcrumb
  background-color white
  padding 0

.tab-content
  padding 15px 0

.list-parent
  li
    position relative
  a
    display block
    &:hover
      text-decoration none
  & > a
    padding-top 3px
    padding-bottom 3px
    padding-right 15px
    padding-left 15px
    font-weight bold
    &.active
      &:before
        font-family "Glyphicons Halflings"
        content ""
        position absolute
        margin-left -18px
        margin-top 0

.list-children
  a
    padding-top 2px
    padding-bottom 2px
    padding-right 15px
    padding-left 30px
    &.active
      &:before
        font-family "Glyphicons Halflings"
        content ""
        position absolute
        margin-left -18px
        margin-top -7px
        top: 50%
        line-height 1

